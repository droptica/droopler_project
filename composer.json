{
	"name": "droptica/droopler-project",
	"description": "Project template for Droopler with composer",
	"type": "project",
	"license": "GPL-2.0+",
	"authors": [{
		"name": "",
		"role": ""
	}],
	"repositories": {
	    "drupal": {
			"type": "composer",
			"url": "https://packages.drupal.org/8"
		},
		"asset": {
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	},
	"require": {
		"composer/installers": "^2.0",
		"cweagans/composer-patches": "^1.6",
		"droptica/d_application_api": "^1.0",
		"droptica/droopler": "^8.3.0",
		"drupal/core-composer-scaffold": "^9",
		"drupal/core-recommended": "^9",
		"drupal/eu_cookie_compliance": "^1.5",
		"drupal/system_status": "^2.8",
		"drush/drush": "^10.6",
		"oomphinc/composer-installers-extender": "^2.0",
		"webflo/drupal-finder": "^1.0.0",
		"webmozart/path-util": "^2.3"
	},
	"require-dev": {
		"behat/mink": "~1.7",
		"behat/mink-goutte-driver": "~1.2",
		"drupal/devel": "^4.1",
		"jcalderonzumba/gastonjs": "~1.0.2",
		"jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
		"mikey179/vfsstream": "~1.2",
		"phpunit/phpunit": "^9",
		"symfony/css-selector": "~2.8|~3.0"
	},
	"conflict": {
		"drupal/drupal": "*"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"scripts": {
		"drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
	},
	"extra": {
		"installer-types": ["bower-asset", "npm-asset"],
		"installer-paths": {
			"web/core": ["type:drupal-core"],
			"web/libraries/{$name}": ["type:drupal-library", "type:git", "type:bower-asset", "type:npm-asset"],
			"web/modules/contrib/{$name}": ["type:drupal-module"],
			"web/profiles/contrib/{$name}": ["type:drupal-profile"],
			"web/themes/contrib/{$name}": ["type:drupal-theme"],
			"drush/contrib/{$name}": ["type:drupal-drush"]
		},
		"drupal-scaffold": {
			"locations": {
				"web-root": "web/"
			}
		},
        "patches": {
            "droptica/droopler": {
                "Temporary fix - entity.manager": "./patches/d_media.patch"
            }
        }
	}
}
